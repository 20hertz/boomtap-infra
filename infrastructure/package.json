{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": "bin/infrastructure.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap:staging": "yarn cdk bootstrap -c config=staging --profile backstage",
    "bootstrap:prod": "yarn cdk bootstrap -c config=staging --profile prod",
    "deploy:staging": "yarn build lambdas/index.ts && cdk deploy LandingPageStack WebAppStack -c config=staging --profile backstage --require-approval never",
    "destroy:all": "cdk destroy --all -c config=staging --profile backstage",
    "deploy:certificate:staging": "cdk deploy CertifiedDomainStack -c config=staging --profile backstage"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.89",
    "@types/jest": "^27.4.0",
    "@types/node": "10.17.60",
    "aws-cdk": "2.5.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.5.0",
    "constructs": "^10.0.29"
  },
  "packageManager": "yarn@3.1.1"
}
