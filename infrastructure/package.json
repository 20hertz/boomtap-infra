{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": "bin/infrastructure.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap:staging": "yarn cdk bootstrap -c config=staging --profile backstage",
    "bootstrap:prod": "yarn cdk bootstrap -c config=prod --profile prod",
    "deploy:staging": "yarn build lambdas/index.ts && cdk deploy --all -c config=staging --profile backstage --require-approval never",
    "destroy:all": "cdk destroy --all -c config=staging --profile backstage",
    "deploy:certificate:staging": "cdk deploy CertifiedDomainStack -c config=staging --profile backstage",
    "deploy:certificate:prod": "cdk deploy CertifiedDomainStack -c config=prod --profile prod"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.107",
    "@types/jest": "^29.1.2",
    "@types/node": "18.11.0",
    "aws-cdk": "2.46.0",
    "jest": "^29.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.5.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.46.0",
    "constructs": "^10.1.131"
  },
  "packageManager": "yarn@3.1.1"
}
