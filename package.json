{
  "name": "boomtap-infra",
  "version": "3.0.0",
  "bin": "bin/boomtap-infra.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:firewall": "cdk deploy FirewallStack -c config=staging --profile boomtap-dev",
    "destroy:firewall": "cdk destroy FirewallStack -c config=staging --profile boomtap-dev",
    "deploy:staging": "cdk deploy FrontEndStackStaging -c config=staging --profile boomtap-dev",
    "destroy:staging": "cdk destroy FrontEndStackStaging -c config=staging --profile boomtap-dev",
    "deploy:landingpage:staging": "cdk deploy LandingPageStack -c config=staging --profile boomtap-dev",
    "destroy:landingpage:staging": "cdk destroy LandingPageStack -c config=staging --profile boomtap-dev",
    "deploy:prod": "cdk deploy FrontEndStackProd -c config=prod --profile boomtap-dev",
    "destroy:prod": "cdk destroy FrontEndStackProd -c config=prod --profile boomtap-dev"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.2.0",
    "@types/jest": "^27.0.3",
    "@types/node": "14.18.2",
    "aws-cdk": "2.2.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.2.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.2.0",
    "constructs": "^10.0.13"
  }
}
