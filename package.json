{
  "name": "boomtap-infra",
  "version": "0.2.0",
  "bin": "bin/boomtap-infra.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:staging": "cdk deploy -c config=staging --profile boomtap-dev",
    "deploy:prod": "cdk deploy -c config=prod --profile boomtap-dev",
    "destroy:staging": "cdk destroy -c config=staging --profile boomtap-dev",
    "destroy:prod": "cdk destroy -c config=prod --profile boomtap-dev"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.117.0",
    "@types/jest": "^26.0.24",
    "@types/node": "14.17.9",
    "aws-cdk": "1.117.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "~4.2.4"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.117.0",
    "@aws-cdk/aws-cloudfront": "^1.117.0",
    "@aws-cdk/aws-iam": "^1.117.0",
    "@aws-cdk/aws-route53": "^1.117.0",
    "@aws-cdk/aws-route53-targets": "^1.117.0",
    "@aws-cdk/aws-s3": "^1.117.0",
    "@aws-cdk/aws-s3-deployment": "^1.117.0",
    "@aws-cdk/core": "1.117.0",
    "source-map-support": "^0.5.19"
  }
}
